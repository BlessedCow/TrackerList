function showSuperV() {
  var data = "john.doe@companyName.org, jane.smith@companyName.org, robert.johnson@companyName.org, sarah.jackson@companyName.org, david.wilson@companyName.org";
  SpreadsheetApp.getUi().alert(data);
}

